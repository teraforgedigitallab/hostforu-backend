{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/api/payment/initiate-cashfree",
      "methods": ["OPTIONS", "POST"],
      "dest": "/api/payment/initiate-cashfree.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://hostforu.com",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-VERIFY, X-MERCHANT-ID",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/payment/verify-cashfree",
      "methods": ["OPTIONS", "POST"],
      "dest": "/api/payment/verify-cashfree.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://hostforu.com",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-VERIFY, X-MERCHANT-ID",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/payment/webhook-cashfree",
      "methods": ["POST"],
      "dest": "/api/payment/webhook-cashfree.js"
    },
    {
      "src": "/api",
      "dest": "/api/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://hostforu.com",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}